@model IEnumerable<MunshiModels.Models.ProcessMasterModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPublic.cshtml";
}
<script src="~/Scripts/jquery-1.12.3.min.js"></script>
<section class="content-header">
    <h1>
        ProcessMasterList
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>ProcessMaster</a></li>
        <li class="active">ProcessMaster List</li>
    </ol>
</section>
<section class="content">
    <div class="row">
        <div class="col-xs-12">
           <a href="@Url.Action("ProcessInser_Update","ProcessMaster")" class="btn btn-block btn-primary" style="width:7%;margin-bottom:1%;"><i class="fa fa-fw fa-plus"></i>Add</a>
            <div class="box box-primary">
                <div class="box-header">
                    <h3 class="box-title">ProcessMaster List</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                @*<th>ProcessId</th>*@
                                <th>ProcessName</th>
                                <th>QuantityFlag</th>
                                <th>Wastage Flag </th>
                                <th>ByProduct</th>
                                @*<th>CreatedDate</th>*@
                                <th>ProcessVolume</th>
                                <th>ProcessDuration</th>
                                
                                <th>Action</th>
                               
                                
                            </tr>
                        </thead>
                        <tbody>
                            
                            @foreach (var item in Model)
                            {
                              <tr>
                                  @*<td>@item.ProcessId</td>*@
                                  <td>@item.ProcessName</td>
                                  <td>@item.QuantityFlag</td>
                                  <td>@item.WastageFlag</td>
                                  <td>@item.ByProduct</td>
                                  @*<td>@item.CreatedDate</td>*@
                                  <td>@item.ProcessDuration</td>
                                  <td>@item.ProcessVolume</td>

                                 
                                 <td><a href="@Url.Action("ProcessInser_Update","ProcessMaster",new {@ProcessId=item.ProcessId })">
                                     <i class="fa fa-edit"></i>Edit</a>
                                     <a href="#" Onclick=fndelete('@item.ProcessId'); id="deletebtn"><i class="fa fa-edit"></i>Delete</a>

                                  </td>
                                  @*<td>
                                      <a href="Url.Action("Process_Delete","ProcessMaster",new {id=item.ProcessId })">
                                          <i class="fa fa-edit"></i>Delete</a></td>*@
                                
                              </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>
</section>
<script src="~/Scripts/bootbox.min.js"></script>
<script>
    $(function () {
        $('#example1').DataTable()
    })
    function fndelete(Id) {
        debugger;
        bootbox.confirm({
            size: "small",
            message: "Are you sure you want to delete?",
            callback: function (result) {
                //var Id= $("#deletebtn").val();
                if (result==true) {
                    var URL = '@Url.Action("Process_Delete", "ProcessMaster")' + "?id=" + Id;
                    window.location.href = URL;
                }
                @*else
                {
                    var URL = '@Url.Action("BuyProductIndex", "BuyProduct")';
                window.location.href = URL;
            }*@

            }
        })
    }
</script>



